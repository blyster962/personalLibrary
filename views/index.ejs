<!DOCTYPE html>
<html>
  <head><% include headerStuff %></head>
  <body>
    <div class="container-fluid">
      <% include pageHeader %>
      <% if (booklist.length > 0) { %>
      <div class="row"><div class="col-xs-12">
        <div class="btn btn-group-vertical" role="group" style="width:100%">
        <% for (var book of booklist) { %>
          <a class="btn btn-lg btn-block btn-default" href="/books/view?key=<%= book.key %>"><%= book.title %></a>
        <% } %>
        </div>
      </div></div>
      <% } %>
      <% include footer %>
    </div>
  </body>
</html>
